{
    "rewrites": [
        {
            "source": "/:path*",
            "destination": "https://sea-lion-app-75389.ondigitalocean.app/:path*"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                }
            ]
        }
    ]
}